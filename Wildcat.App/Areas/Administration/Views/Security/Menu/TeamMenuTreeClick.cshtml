@model Wildcat.App.ViewModels.TeamTreeClickViewModel
<div id="id_subscription-@Model.navmenu.Id" class="card mb-4">
    <h6 class="card-header">@Model.navmenu.DisplayName - @Model.navmenu.ControllerName/@Model.navmenu.ActionName</h6>
    <div class="card-body" id="locationsCard">
        <ul id="sortable1" class="connectedSortable">
            @foreach (var m in Model.securityGroups)
            {
                <li>
                    @{var idd = $"{Model.MenuId}|{m.SecurityName}";}
                    <input id="@idd" class="form-check-input" type="checkbox" value="@m.SecurityName" @(Model.menuteams.menuSecurityGroups.Contains(m) ? "checked" : "") onclick="changeMenuTeamStatus(this)">
                    <div class="form-check-label">@m.SecurityName</div>
                </li>
            }
        </ul>
    </div>
</div>
