<script type="text/javascript">

    $(function () {
        $("#DieNo").autocomplete({
            source: function (request, response) {
                $.ajax({
                    url: '/MCNJ/HeatTreatBatchRecords/MetRecord/AutoCompleteDies?prefix=' + request.term,
                    dataType: "json",
                    type: "POST",
                    contentType: "application/json; charset=utf-8",
                    success: function (data) {
                        response($.map(data, function (item) {
                            return item;
                        }))
                    },
                    error: function (response) {
                        alert(response.responseText);
                    },
                    failure: function (response) {
                        alert(response.responseText);
                    }
                });
            },
            select: function (e, i) {
                $('#MillIdVal').val(i.item.val)
            },
            minLength: 1
        });
    });

    function editheatbtn(e) {
        var id = e;
        $.blockUI({
            message: '<div class="sk-folding-cube sk-primary"><div class="sk-cube1 sk-cube"></div><div class="sk-cube2 sk-cube"></div><div class="sk-cube4 sk-cube"></div><div class="sk-cube3 sk-cube"></div></div><h5 style="color: #444">LOADING...</h5>',
            css: {
                backgroundColor: 'transparent',
                border: '0',
                zIndex: 9999999
            },
            overlayCSS: {
                backgroundColor: '#fff',
                opacity: 0.8,
                zIndex: 9999990
            }
        });

        $.ajax({
                url: "/MCNJ/HeatTreatBatchRecords/MetRecord/EditHeatPartial/?ID=" + encodeURIComponent(id),
                type: "GET",
                cache: false,
                success: function (result) {
                    $.unblockUI();

                    $('#modals-body-edit').html(result.Form.Result);
                    $('#modals-default-edit').modal('show');
                },
                error: function (result) {
                    $.unblockUI();

                    $.growl.error({
                        message: result.statusText
                    });
                }

            });
        return false;
    };

    $('#FormSubmit').click(function (e) {
        var id = e.currentTarget.dataset.value;
        $.blockUI({
            message: '<div class="sk-folding-cube sk-primary"><div class="sk-cube1 sk-cube"></div><div class="sk-cube2 sk-cube"></div><div class="sk-cube4 sk-cube"></div><div class="sk-cube3 sk-cube"></div></div><h5 style="color: #444">LOADING...</h5>',
            css: {
                backgroundColor: 'transparent',
                border: '0',
                zIndex: 9999999
            },
            overlayCSS: {
                backgroundColor: '#fff',
                opacity: 0.8,
                zIndex: 9999990
            }
        });

        var RowId = $('#RowId').val();
        var DieNo = $('#DieNo').val();
        var OrderNo = $('#OrderNo').val()
        var QuantityRun = $('#QuantityRun').val();
        var TestBars = $('#TestBars').val();
        var TotalFurnaceTime = $('#TotalFurnaceTime').val();
        var QuenchTempStart = $('#QuenchTempStart').val();
        var QuenchTempFinish = $('#QuenchTempFinish').val();
        var Notes = $('#Notes').val();
        var ApprovedBy = $('#ApprovedBy').val();
        var SectionSize = $('#SectionSize').val();
        var Weight = $('#Weight').val();

        $.ajax({
            url: "/MCNJ/HeatTreatBatchRecords/MetRecord/EditMetResult",
            type: "POST",
            data: {
                RowId: RowId,
                DieNo: DieNo,
                OrderNo: OrderNo,
                QuantityRun: QuantityRun,
                TestBars: TestBars,
                TotalFurnaceTime: TotalFurnaceTime,
                QuenchTempStart: QuenchTempStart,
                QuenchTempFinish: QuenchTempFinish,
                Notes: Notes,
                ApprovedBy: ApprovedBy,
                SectionSize: SectionSize,
                Weight: Weight
            },
            cache: false,
            success: function (result) {
                $.unblockUI();

                if (result.success) {
                    $.growl.notice({
                        message: result.message
                    });
                    $('#RowId').val(result.Record.RowId);
                    $('#OrderNo').val(result.Record.OrderNo);
                    $('#AddOrderNo').val(result.Record.OrderNo);
                    $('#htoperations').html(result.Form.Result);

                    //datatable();
                    //var mytbl = $("#m_table_2").DataTable();
                    //mytbl.ajax.reload(null, false);
                }
                else {
                    $.growl.error({
                        message: result.message
                    });
                }
            },
            error: function (result) {
                $.unblockUI();

                $.growl.error({
                    message: result.statusText
                });
            }

        });

        return false;

    });

    $('#NewOpSave').click(function (e) {
        $.blockUI({
            message: '<div class="sk-folding-cube sk-primary"><div class="sk-cube1 sk-cube"></div><div class="sk-cube2 sk-cube"></div><div class="sk-cube4 sk-cube"></div><div class="sk-cube3 sk-cube"></div></div><h5 style="color: #444">LOADING...</h5>',
